<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.insikostmobile">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".root.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InsikostMobile">
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.berita.Berita"
            android:exported="false" />
        <activity
            android:name=".ui.pelaporan.Pelaporan"
            android:exported="false" />
        <activity
            android:name=".ui.berita.ListBerita"
            android:exported="false" />
        <activity
            android:name=".ui.pelaporan.InputPelaporan"
            android:exported="false" />
        <activity
            android:name=".ui.pelaporan.ListPelaporan"
            android:exported="false" />
        <activity
            android:name=".ui.pembayaran.ListPembayaran"
            android:exported="false" />
        <activity
            android:name=".ui.pembayaran.InputPembayaran"
            android:exported="false" />
        <activity
            android:name="com.example.insikostmobile.ui.profile.ProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.example.insikostmobile.ui.pembayaran.Pembayaran"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>